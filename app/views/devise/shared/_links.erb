<%- if controller_name != 'sessions' %>
  <%= link_to "Logg inn", new_session_path(resource_name), :style => "color:white",:class => "btn btn-primary" %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Opprett konto", new_registration_path(resource_name), :style => "color:white", :class => "btn btn-warning btn-small" %><br />
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' %>
  <%= link_to "Glemt passord?", new_password_path(resource_name), :style => "color:white",:class => "btn btn-info btn-small" %><br />
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Har ikke f책tt bekreftelse?", new_confirmation_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Har ikke f책tt instruksjoner for 책 l책se opp konto?", new_unlock_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Logg inn med #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider) %><br />
  <% end -%>
<% end -%>